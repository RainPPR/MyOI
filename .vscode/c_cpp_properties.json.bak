{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include/**"
            ],
            "defines": [
                "MY_TEST=1"
            ],
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerPath": "/usr/bin/g++",
            "compilerArgs": [
                "-fdiagnostics-color=always -O2 -lm -std=c++14 -Wall -Wextra -Wshadow -Wconversion -pedantic -fno-asm -fno-ms-extensions -fno-omit-frame-pointer -g -pipe -s -static"
            ]
        }
    ],
    "version": 4
}