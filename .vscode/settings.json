{
	"git.enableSmartCommit": true,
	"git.autofetch": true,
	"files.autoGuessEncoding": true,
	"code-runner.fileDirectoryAsCwd": true,
	"code-runner.runInTerminal": true,
	"code-runner.saveAllFilesBeforeRun": true,
	"code-runner.executorMap": {
		"python": "python -u",
        "pascal": "fpc \"$fileName\" -O2\ntime \"./$fileNameWithoutExt\" && rm -f \"./$fileNameWithoutExt\" && rm -f \"./$fileNameWithoutExt.o\"",
		"c": "gcc \"$fileName\" -O2 -Wall -Wextra -DONLINE_JUDGE",
		"cpp": "g++ \"$fileName\" -fdiagnostics-color=always -Ofast -ffast-math -std=c++17 -Wall -Wextra -Wconversion -pedantic -fno-asm -fno-ms-extensions -fno-omit-frame-pointer -pipe -s -static && echo Accepted! && ./a.out"
	},
    "editor.mouseWheelZoom": true,
    "editor.wordWrap": "off",
    "files.encoding": "utf8",
    "C_Cpp.default.compilerPath": "",
    "C_Cpp.vcFormat.indent.accessSpecifiers": true,
    "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
    "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyType": true,
    "C_Cpp.vcFormat.space.betweenEmptyBraces": true,
    "C_Cpp.vcFormat.space.betweenEmptyParameterListParentheses": true,
    "C_Cpp.vcFormat.space.betweenEmptySquareBrackets": true,
    "C_Cpp.vcFormat.space.pointerReferenceAlignment": "right",
    "C_Cpp.vcFormat.space.removeAroundUnaryOperator": false,
    "C_Cpp.autocompleteAddParentheses": true,
    "C_Cpp.inlayHints.autoDeclarationTypes.showOnLeft": true,
    "C_Cpp.inlayHints.parameterNames.suppressWhenArgumentContainsName": false,
    "C_Cpp.default.cppStandard": "c++14",
    "C_Cpp.default.cStandard": "c11",
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.autoImportCompletions": true,
    "files.associations": {
